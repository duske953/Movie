{
  "name": "movies",
  "version": "1.0.0",
  "description": "A web app to get information about the latest movies and tv shows",
  "targets": {
    "default": {
      "distDir": "./public"
    }
  },
  "scripts": {
    "start": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "parcel-dev": "parcel watch ./public/js/script.js",
    "parcel-prod": "parcel build ./public/js/script.js",
    "sass-dev": "sass --watch --update --style=expanded sass/main.scss:public/css/styles.css",
    "sass-prod": "sass --no-source-map --style=compressed sass/main.scss:public/css/styles.css"
  },
  "keywords": [],
  "author": "Eloho Kennedy",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.5",
    "express": "^4.18.1",
    "express-minify-html": "^0.12.0",
    "jquery": "^3.7.1",
    "lozad": "^1.16.0",
    "morgan": "^1.10.0",
    "slick-carousel": "^1.8.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "parcel": "^2.6.2"
  }
}
